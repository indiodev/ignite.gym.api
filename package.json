{
	"name": "03-solid",
	"version": "1.0.0",
	"description": "GymPass Style App.",
	"main": "index.js",
	"scripts": {
		"dev": "tsx watch bin/server.ts",
		"start": "node build/bin/server.js",
		"test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
		"test:install-prisma-environment": "npm link vitest-environment-prisma",
		"build": "tsup --ignore-watch node_modules app bin start config --out-dir build",
		"test": "vitest run --dir app/use-cases",
		"test:e2e": "vitest run --dir app/controllers",
		"test:e2e:watch": "vitest watch --dir app/controllers",
		"prestest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
		"test:watch": "vitest --dir app/use-cases",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "22.9.0",
		"@types/supertest": "6.0.2",
		"@typescript-eslint/eslint-plugin": "8.13.0",
		"@typescript-eslint/parser": "8.13.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-import-resolver-alias": "1.1.2",
		"eslint-plugin-import": "2.31.0",
		"eslint-plugin-n": "17.13.1",
		"eslint-plugin-prettier": "5.2.1",
		"eslint-plugin-promise": "7.1.0",
		"npm-run-all": "4.1.5",
		"prettier": "3.3.3",
		"prisma": "5.22.0",
		"supertest": "7.0.0",
		"tsup": "8.3.5",
		"tsx": "4.19.2",
		"typescript": "5.6.3",
		"vitest": "2.1.4"
	},
	"dependencies": {
		"@fastify/cookie": "11.0.1",
		"@fastify/jwt": "9.0.2",
		"@prisma/client": "5.22.0",
		"@types/bcryptjs": "2.4.6",
		"@vitest/coverage-v8": "2.1.4",
		"bcryptjs": "2.4.3",
		"dayjs": "1.11.13",
		"dotenv": "16.4.5",
		"fastify": "5.1.0",
		"zod": "3.23.8"
	}
}
